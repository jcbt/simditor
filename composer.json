{
    "name": "jcbt/simditor",
    "description": "Integrates simditor into Laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "simditor"],
    "homepage": "https://github.com/jcbt/simditor",
    "license": "MIT",
    "authors": [
        {
            "name": "gaowei",
            "email": "admin@gaowei.co"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Jcbt\\Simditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jcbt\\Simditor\\SimditorServiceProvider"
            ]
        }
    }
}
